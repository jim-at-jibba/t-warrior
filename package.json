{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "check-formatting": "prettier --config .prettierrc 'src/**/*.tsx' '**/*.tsx' --list-different",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.tsx' '**/*.tsx' --write",
    "lint-src": "eslint 'src/**' --ext .js,.jsx,.ts,.tsx",
    "lint": "npm run lint-src && npm run check-ts-types",
    "check-ts-types": "tsc -p ./tsconfig.json --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "expo": "~42.0.1",
    "expo-status-bar": "~1.0.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-web": "~0.13.12"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-universe": "^7.0.1",
    "fit-commit-js": "^0.3.2",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "~4.0.0"
  },
  "private": true,
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "eslint --ext .js,.ts,.tsx,.jsx",
      "prettier --list-different"
    ]
  }
}
