{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@Components/*": [
        "./src/components/*"
      ],
      "@Utils/*": [
        "./src/utils/*"
      ],
      "@Hooks/*": [
        "./src/hooks/*"
      ],
      "@Types/*": [
        "./src/types/*"
      ],
      "@Constants/*": [
        "./src/constants/*"
      ],
      "@Store/*": [
        "./src/store/*"
      ],
      "@Features/*": [
        "./src/features/*"
      ],
      "@Nav/*": [
        "./src/navigation/*"
      ]
    }
  }
}
